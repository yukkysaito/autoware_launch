<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="vehicle_model" default="ymc_golfcart_proto2" />
  <arg name="sensor_model" default="aip_x1" />
  <arg name="map_path" default="/opt/autoware/maps/" />
  <arg name="lanelet_map_file" default="lanelet2_map.osm" />
  <arg name="pointcloud_map_file" default="pointcloud_map_file" />
  <arg name="rviz" default="false" />

  <include file="$(find autoware_launch)/launch/autoware.launch.xml">
    <arg name="vehicle_model" value="$(var vehicle_model)"/>
    <arg name="sensor_model" value="$(var sensor_model)"/>
    <arg name="map_path" value="$(var map_path)"/>
    <arg name="lanelet2_map_file" value="$(var lanelet_map_file)"/>
    <arg name="pointcloud_map_file" value="$(var pointcloud_map_file)"/>
    <arg name="rviz" value="$(var rviz)"/>
  </include>
</launch>
